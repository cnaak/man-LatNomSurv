@Article{2012-AmbrusVE+SofoneaV-PhysRevE,
  author    = {Ambrus, Victor Eugen and Sofonea, Victor},
  title     = {High-order thermal lattice Boltzmann models derived by means of Gauss quadrature in the spherical coordinate system},
