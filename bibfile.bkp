@Article{2007-SiebertDN+PhilippiPC-IntJModPhysC,
  author    = {Diogo Nardelli Siebert and Hegele, Jr., Luiz Adolfo and Rodrigo Surmas and dos Santos, Lu\'{\i}s Orlando Emerich and Paulo Cesar Philippi},
  title     = {Thermal lattice-Boltzmann in two dimensions},
  journal   = {International Journal of Modern Physics C},
